#include <stdio.h>

int main()
{	
	int n = 0;
	scanf("%d", &n);
	
	for(int a = 0; a * a <= n; a++)
	{
		for(int b = 0; b * b <= n; b++)
		{
			for(int c = 0; c * c <= n; c++)
			{
				for(int d = 0; d * d <= n; d++){
					if(a*a+b*b+c*c+d*d==n)
					{
						printf("%d %d %d %d", a, b, c, d);
						return 0;
					}
				}
			}
		}
	}
	
		
	return 0;
}
